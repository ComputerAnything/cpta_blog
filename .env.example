# Docker Compose environment variables
# Copy this file to .env and fill in your values
# This file is used by docker-compose.yml and docker-compose.staging.yml

# Redis password (must match backend/.env REDIS_PASSWORD)
# Generate with: openssl rand -base64 32
# Development: Use "changeme" for convenience
# Staging/Production: Use strong random password
REDIS_PASSWORD=your_redis_password_here
